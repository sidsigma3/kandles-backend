{ "version": 2,
"routes":[{"src":"/(.*)","dest":"/"}],
"builds": [{"src":"server.js","use":"@vercel/node"}],
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/https://kandles-ui.vercel.app/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }  
]

}

